F = (G M m) / (R^2)
	F = gravitational force between bodies [kg m / s^2], M = mass of body 1 [kg], m = mass of body 2 [kg], R = distance between bodies [m]
	If solving for:
		F -- takes M, m, R
		M -- takes F, m, R
		m -- takes F, M, R
		R -- takes F, M, m
b = L / (4 Pi d^2)
	b = brightness of source [W / m^2], L = luminosity of source [W], d = distance to source [m]
	If solving for:
		b -- takes L, d
		L -- takes b, d
		d -- takes b, L
m - M = 5 log_10(d - 5)
	m = apparent magnitude, M = absolute magnitude, d = distance to object [pc]
	If solving for:
		m -- takes M, d
		M -- takes m, d
		d -- takes m, M
d / d_sun = sqrt((L / L_sun) / (b / b_sun))
	L = Luminosity of source [W], b = brightness of source [W / m^2], d = distance to source [m]
	If solving for:
	 d -- takes L, b
	 L -- takes d, b
	 b -- takes d, L
delta_lambda / lambda_0 = v / c
	delta_lambda = change in wavelength [m], lambda_0 = rest wavelength [m], v = velocity of object [m / s]
	If solving for:
		delta_lambda -- takes lambda_0, v
		lambda_0 -- takes delta-lambda, v
		v -- takes delta_lambda, lambda_0
E = h nu
	E = energy pf photon [J], nu = frequency of photon [Hz]
	If solving for:
		E -- takes nu
		nu -- takes E
E = h c / lambda
	E = energy pf photon [J], lambda = wavelength of photon [m]
	If solving for:
		E -- takes lambda
		lambda -- takes E
v = H_0 d
	v = recessional velocity of object [km / s], d = distance of object [Mpc]
	If solving for:
		v -- takes d
		d -- takes v
z = (lambda - lambda_0) / lambda_0
	z = redshift, lambda = observed wavelength [m], lambda_0 = rest wavelength [m]
	If solving for:
		z -- takes lambda, lambda_0
		lambda -- takes z, lambda_0
		lambda_0 -- takes z, lambda
z = v / c
	z = redshift, v = velocity [m / s]
	If solving for:
		z -- takes v
		v -- takes z
KE = (1 / 2) m v^2
	KE = kinetic energy [J], m = mass [kg], v = velocity [m / s]
	If solving for:
		KE -- takes m, v
		m -- takes KE, v
		v -- takes KE, m
v = sqrt((3 k T) / m)
	v = velocity [m / s], T = temperature [K], m = mass [kg]
	If solving for:
		v -- takes T, m
		T -- takes v, m
		m --takes v, T
m_2 - m_1 = 2.5 log(b_2 / b_1)
	m_1 = apparent magnitude of object 1, m_2 = apparent magnitude of object 2, b_1 = brightness of object 1 [W / m^2], b_2 = brightness of object 2 [W / m^2]
	If solving for:
		m_1 -- takes m_2, b_1, b_2
		m_2 -- takes m_1, b_1, b_2
		b2 -- takes m_1, m_2, b_2
		b1 -- takes m_1, m_2, b_1
B - V_color = m_B - m_V
	B = blue, V_color = visible color, m_B = apparent blue magnitude, m_V = apparent visible magnitude
	If solving for:
		B -- takes V_color, m_B, m_V
		V_color -- takes B, m_B, m_V
		m_b
B - V_color = -2.5 log(b_B / b_V)
	B = blue, V_color = visible color, b_B = blue brightness [W / m^2], b_V = visible brightness [W / m^2]
	If solving for:
		B -- takes V_color, b_B, b_V
		V_color -- takes B, b_B, b_V
		b_B -- takes B, V_color, b_V
		b_V -- takes B, V_color, b_B
E = m c ^2
	E = total energy [J], m = mass [kg]
	If solving for:
		E -- takes m
		m -- takes E
P^2 = a^3
	P = orbital period [yr], a = semimajor axis of orbit [au]
	If solving for:
		P -- takes a
		a -- takes P
P^2 = ((4 Pi^2) / (G (m_1 + m_2))) a^3
	P = orbital perion [s], a = semimajor axis of orbit [m], m_1 = mass of body 1 [kg], m_2 = mass of body 2 [kg]
	If solving for:
		P -- takes m_1, m_2, a
		m_1 -- takes P, m_2, a
		m_2 -- takes P, m_1, a
		a -- takes P, m_1, m_2
d = 1 / p
	d = distance [pc], p = parallax angle [arcsec]
	If solving for:
		d -- takes p
		p -- takes d
z = sqrt((c + v) / (c - v)) - 1
	z = redshift, v = velocity [m/s]
	If solving for:
		z -- takes v
		v -- takes z
T = t_0 / sqrt(1 - (v / c)^2)
	T = time in moving frame [s], T_0 = time in rest frame [s], v = velocity [m /s]
	If solving for:
		T -- takes t_0, v
		t_0 -- takes T, v
		v -- takes T, t_0
L = L_0 sqrt(1 - (v / c)^2)
	L = length in moving frame [s], L_0 = length in rest frame [s], v = velocity [m /s]
	If solving for:
		L -- takes L_0, v
		L_0 -- takes L, v
		v -- takes L, L_0
R_Sch = (2 G m) / c^2
	R_Sch = Schwarzchild radius [m], m = mass of body [m]
	If solving for:
		R_Sch -- takes m
		m -- takes R_Sch
L = 4 Pi R^2 sigma T^4
	L = luminosidy [W], R = radius of object [m], T = temperature of object [K]
	If solving for:
		L -- takes R, T
		R -- takes L, T
		T -- takes L, R
F = sigma T^4
	F = flux of object [W / m^2], T = temperature of object [K]
	If solving for:
		F -- takes T
		T -- takes F
Magnification = f / D
	Magnification = telescope magnification, f = telescope focal length [m], D = telescope diameter [m]
	If solving for:
		Magnification -- takes f, D
		f -- takes Magnification, D
		D -- takes Magnification, f
Resolution = 116 / D
	Resolution = resolution of telescope, D = diameter of telescope [mm]
	If solving for:
		Resolution -- takes D
		D -- takes Resolution
w = f theta Pi / 180
	w = width of object [m], f = focal length of telescope [m], theta = apparent diameter of object [degrees]
	If solving for:
		w -- takes f, theta
		f -- takes w, theta
		theta -- takes w, f
m = 2.7 + 5 log(D)
	m = limiting magnitude, D = diameter of telescope [mm]
	If solving for:
		m -- takes D
		D -- takes m
F_tidal = F_near - F_far
	F_tidal = tidal force on object [kg m / s^2], F_near = near force on object [kg m / s^2], f_far = far force on object [kg m / s^2]
	If solving for:
		F_tidal -- takes F_near, F_far
		F_near -- F_tidal, F_far
		F_far -- F_tidal, F_near
F_tidal = (2 G M m d) / r^3
	F_tidal = tidal force on object [kg m / s^2], M = mass of object exerting tidal force, m = mass of object force is exerted on, d = distance between object, r = radius of object force is exerted on
	If solving for:
		F-tidal -- takes M, m, d, r
		M -- takes F_tidal, m, d, r
		m -- takes F_tidal, M, d, r
		r -- takes F_tidal, M, m, d
lambda_max = 0.0029 / T
	lambda_max = peak blackbody wavelength [m], T = temperture of object [K]
	If solving for:
		lambda_max -- takes T
		T -- takes lambda_max
